#!/usr/bin/env ruby

require 'faker'

puts Faker::Name.name
puts
puts Faker::Internet.email
puts Faker::Company.name #=> "Hirthe-Ritchie"
puts
puts Faker::Address.street_address #=> "282 Kevin Brook"
puts Faker::Address.city #=> "Imogeneborough"
puts Faker::Address.zip_code #=> "58517"
puts Faker::Address.state #=> "California"
puts
puts Faker::Internet.password(10, 20) #=> "eoc9shwd1hwq4vbgfw"
